<?xml version="1.0" encoding="UTF-8"?>

 <!DOCTYPE hibernate-mapping PUBLIC
 "-//Hibernate/Hibernate Mapping DTD 3.0//En"
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="hib.dto">
    <class name="BillingCounter1" table="billingcounter">
        <id name="regNo" column="registrationNo"/>
        <property name="patientName" column="patientName"/>
        <property name="diseaseName" column="diseaseName"/>
        <property name="paymentMode" column="payment"/>
        <property name="amount" column="amount"/>
    </class>
</hibernate-mapping>


