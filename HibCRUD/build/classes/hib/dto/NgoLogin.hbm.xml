<?xml version="1.0" encoding="UTF-8"?>

 <!DOCTYPE hibernate-mapping PUBLIC
 "-//Hibernate/Hibernate Mapping DTD 3.0//En"
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="hib.dto">
    <class name="NgoLogin" table="ngoinfo">
        <id name="loginId" column="loginId"/>
        <property name="ngoName" column="ngoName"/>
        <property name="ngoId" column="ngoId"/>
        <property name="password" column="pass"/>
        <property name="contact" column="contact"/>
        <property name="address" column="address"/>
        <property name="ngoEstablishedYear" column="ngoEstablishedYear"/>
        <property name="dateOfRegistration" column="dateOfRegistration"/>
        <property name="city" column="city"/>
        <property name="email" column="email"/>
    </class>
    </hibernate-mapping>
