<?xml version="1.0" encoding="UTF-8"?>

 <!DOCTYPE hibernate-mapping PUBLIC
 "-//Hibernate/Hibernate Mapping DTD 3.0//En"
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="hib.dto">
    <class name="LabDetail" table="labinfo">
        <id name="labNo" column="labNo"/>
        <property name="labName" column="labName"/>
        <property name="floorNo" column="floorNo"/>
        <property name="labDetails" column="labDetails"/>
    </class>
</hibernate-mapping>


