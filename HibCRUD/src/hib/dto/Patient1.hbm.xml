<?xml version="1.0" encoding="UTF-8"?>

 <!DOCTYPE hibernate-mapping PUBLIC
 "-//Hibernate/Hibernate Mapping DTD 3.0//En"
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="hib.dto">
    <class name="Patient1" table="patient1table">
        <id name="regNo" column="registrationNo"/>
        <property name="patientName" column="patientName"/>
        <property name="contactNo" column="contactNo"/>
        <property name="dob" column="dob"/>
        <property name="gender" column="gender"/>
        <property name="status" column="status"/>
        <property name="address" column="address"/>
        <property name="city" column="city"/>
        <property name="bloodGroup" column="bloodGroup"/>
        <property name="diseaseName" column="diseaseName"/>
        
    </class>
</hibernate-mapping>


